    puts(str);
    int index=0;
    while(str[index]!='\0')
    {
    index++;
    }
    printf("%d",index-1);
    return 0;
9.separate the character from the string
#include <stdio.h>
#include <string.h>
int main() {
    // Write C code here
    char str[30];
    fgets(str,sizeof(str),stdin);
    puts(str);
    int index=0;
    while(str[index]!='\0')
    {
    printf("%c ",str[index]);
    index++;
    }
    return 0;
}
10.print the individual character in reverse order
#include <stdio.h>
#include <string.h>
int main() {
    // Write C code here
    char str[30];
    fgets(str,sizeof(str),stdin);
    puts(str);
    int index=0;
    while(str[index]!='\0')
    index++;
    for(int i=index-2;i>=0;i--)
    {    
    printf("%c ",str[i]);

    }
    return 0;
}
